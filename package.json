    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "start-server-and-test": "^1.12.6",
    "ts-node": "^10.1.0",
